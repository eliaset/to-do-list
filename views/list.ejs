<%- include("header") %>
<body>
  <div class="box" id="heading">
    <h1><%= listTitle %></h1>
  </div>

  <div class="box">
    <% item.forEach((element) => { %>
    <div class="item">
      <input type="checkbox" />
      <p><%= element %></p>
    </div>
    <% }); %>

    <form class="item" action="/" method="post">
      <input type="text" id="addList" name="addList" placeholder="New Item" />
      <button type="submit" name="list" value="<%= listTitle %>">+</button>
    </form>
  </div>
</body>

<%- include("footer") %>
